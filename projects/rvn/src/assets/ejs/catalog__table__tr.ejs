<div class="catalog__table__tr <% if (locals.isRed) { %>	catalog__table__tr--red	<% } %>">
	<% if ([1, 2, 3].includes(locals.type)) { %>
	<div class="catalog__table__td catalog__table__td--index">
		<div class="catalog__table__index"> </div>
	</div>
	<% } %>
	<div class="catalog__table__td  catalog__table__td--img">
		<% if (!locals.isRed) { %>
		<!-- <div class="catalog__table__td__label catalog__table__td__label--light"> -->
		<div class="catalog__table__td__labels">
			<div class="catalog__table__td__label catalog__table__td__label--<%- locals.labelColor %>">
				<span> <%- locals.labelText %> </span>
			</div>
			<div class="catalog__table__td__label catalog__table__td__label--<%- locals.labelColor %>">
				<span> <%- locals.labelText %> </span>
			</div>
		</div>
		<div class="catalog__table__img">
			<img src="<% if (locals.index != 2) { %>
			 /src/assets/static/img/3.webp
			<% } else { %>
				/src/assets/static/img/nophoto.svg <% } %>" alt="">
		</div>
		<% } %>
	</div>
	<div class="catalog__table__td  catalog__table__td--article">
		<div class="catalog__table__article">
			<span>311302832</span>
		</div>
	</div>
	<div class="catalog__table__td  catalog__table__td--name">
		<div class="catalog__table__name">
			<span>DS-2CD2043G0-I (2.8mm) 4Мп уличная цилиндрическая IP-камера с EXIR-подсветкой до 30м</span>
		</div>
	</div>
	<% if (!locals.isRed) { %>
	<% if (locals.type == 2) { %>
	<div class="catalog__table__td catalog__table__td--counter">
		<div class="catalog__table__counter">
			<div class="quantity <% if (locals.noInit) { %> quantity--no-init <% } %>">
				<div class="quantity__button quantity__button--subtract"></div>
				<input type="number" class="quantity__input" step="1" min="0" value="<%- locals.value ?? 0 %>" max="5">
				<div class="quantity__button quantity__button--add"></div>
			</div>
			<% if (!locals.hidePreorder) { %>
			<span class="catalog__table__counter__warning">В предзаказ</span>
			<% } %>
		</div>
	</div>
	<% } else { %>
	<div class="catalog__table__td catalog__table__td--price1">
		<div class="catalog__table__price">
			<span class="catalog__table__price__current">490 000 <span class="font--ruble">₽</span> </span>
			<span class="catalog__table__price__old">500 300 <span class="font--ruble"> ₽</span></span>
		</div>
	</div>
	<% } %>
	<% } else { %>
	<div class="catalog__table__td catalog__table__td--price1">
		<div class="catalog__table__price">
			<span class="catalog__table__price__current">Не найден</span>
		</div>
	</div>
	<% } %>
	<div class="catalog__table__td catalog__table__td--price2">
		<% if (!locals.isRed) { %>
		<div class="catalog__table__price">
			<span class="catalog__table__price__current">490 000 <span class="font--ruble">₽</span></span>
			<span class="catalog__table__price__old">500 300 <span class="font--ruble">₽</span></span>
			<span class="catalog__table__price__bonus">+ 10 000 бонусов</span>
		</div>
		<% } %>
	</div>
	<% if (!locals.isRed) { %>
	<% if (locals.type == 2) { %>
	<div class="catalog__table__td catalog__table__td--price2">
		<div class="catalog__table__price">
			<span class="catalog__table__price__current">490 000 <span class="font--ruble">₽</span></span>
			<span class="catalog__table__price__old">500 300 <span class="font--ruble">₽</span></span>
		</div>
	</div>
	<% } else { %>
	<div class="catalog__table__td catalog__table__td--counter">
		<div class="catalog__table__counter">
			<div class="quantity  <% if (locals.noInit) { %> quantity--no-init <% } %>">
				<div class="quantity__button quantity__button--subtract"></div>
				<input type="number" class="quantity__input" step="1" min="0" value="<%- locals.value ?? 0 %>" max="5">
				<div class="quantity__button quantity__button--add"></div>
			</div>
			<% if (!locals.hidePreorder) { %>
			<span class="catalog__table__counter__warning">В предзаказ</span>
			<% } %>
		</div>
	</div>
	<% } %>
	<% } else { %>
	<div class="catalog__table__td"></div>
	<% } %>
	<% if ([1, 2, 3].includes(locals.type)) { %>
	<% if (!locals.isRed) { %>
	<% if (locals.type == 2) { %>
	<div class="catalog__table__td catalog__table__td--price-input">
		<input type="number" placeholder="100 500">
		<b><span class="font--ruble">₽</span></b>
	</div>
	<% } else { %>
	<div class="catalog__table__td catalog__table__td--price3">
		<div class="catalog__table__price">
			<span class="catalog__table__price__current">490 000 <span class="font--ruble">₽</span></span>
			<span class="catalog__table__price__bonus">+ 10 000 бонусов</span>
		</div>
	</div>
	<% } %>
	<% } else { %>
	<div class="catalog__table__td"></div>
	<% } %>
	<% } %>
	<div class="catalog__table__td catalog__table__td--availability">
		<% if (!locals.isRed) { %>
		<div class="catalog__table__availability">
			<p><span>на Складе по умолчанию:</span> <span>нет (прибудет на склад 22.12)</span> </p>
			<p> <span>на Центральном складе:</span><span>нет (прибудет на склад 22.12)</span> </p>
		</div>
		<% } %>
	</div>
	<% if ([1, 2].includes(locals.type)){ %>
	<div class="catalog__table__td catalog__table__td--remove">
		<div class="catalog__table__remove"></div>
	</div>
	<% } %>
</div>